{
  "name": "league-stats",
  "productName": "League Stats",
  "version": "1.0.0",
  "description": "An app that calculates stats about the best bans in League of Legends",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/electron.icns --prune=true --out=release-builds",
    "package-windows": "electron-packager . league-stats --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='League Stats'"
  },
  "author": "Avery Vine",
  "devDependencies": {
    "electron": "^1.7.9",
    "electron-packager": "^10.1.0"
  },
  "dependencies": {
    "jquery": "^3.2.1"
  }
}
