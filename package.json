{
    "name": "league-stats",
    "productName": "League Stats",
    "version": "1.0.0",
    "description": "This is an Electron-based application that provides players with detailed stats during the pre-game phase. These stats include pick/win/ban rates for all champions, and % advantage (winrate) gained by banning particular champions.",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "build": "electron ./node_modules/electron-toolkit"
    },
    "author": "Avery Vine",
    "devDependencies": {
        "electron": "^2.0.0",
        "electron-builder": "^20.27.1",
        "electron-toolkit": "^1.0.24",
        "jshint": "^2.9.6"
    },
    "dependencies": {
        "jquery": "^3.2.1"
    },
    "build": {
        "appId": "com.electron.league-stats",
        "copyright": "Copyright Â© 2018 Avery Vine",
        "productName": "League Stats",
        "electronVersion": "2.0.8",
        "asar": true,
        "directories": {
            "app": "/Users/AveryVine/Documents/Code/Electron/League Stats",
            "output": "dist"
        },
        "mac": {
            "category": "public.app-category.games",
            "target": [
                {
                    "target": "dmg",
                    "arch": [
                        "x64"
                    ]
                }
            ]
        }
    }
}